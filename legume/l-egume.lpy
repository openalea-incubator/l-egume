    newage = age+STEPS
    lpet = sh.calc_Lpet(ParamP[nump], rank, rankp, ordre, l, type=ParamP[nump]['type'])#m
    
    vox = riri.WhichVoxel(array([X0,Y0,Z0]), origin_grid, na, dxyz)
